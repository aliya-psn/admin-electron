{
  "name": "Swhy",
  "version": "1.0.0",
  "type": "module",
  "description": "Swhy应用描述",
  "main": "electron/main.js",
  "author": "Swhy",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview:prod": "yarn build && vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{src,tests,types}/**/*.{vue,js,jsx,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"{src,tests,types}/**/*.{vue,js,jsx,ts,tsx,json,css,less,scss,html,md}\"",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "commit": "git-cz",
    "analyze": "vite build --mode analyze",
    "clean": "rimraf dist dist_electron",
    "preelectron:build": "yarn clean && vue-tsc --noEmit && vite build",
    "electron:dev": "npm-run-all --parallel dev electron:start",
    "electron:start": "wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .",
    "electron:build": "yarn gen:icon && yarn preelectron:build && electron-builder",
    "electron:build:mac": "yarn gen:icon && yarn preelectron:build && electron-builder --mac",
    "electron:build:win": "yarn gen:icon && yarn preelectron:build && electron-builder --win",
    "electron:build:linux": "yarn gen:icon && yarn preelectron:build && electron-builder --linux",
    "gen:icon": "node scripts/gen-icon.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@icon-park/vue-next": "^1.4.2",
    "@types/js-cookie": "^3.0.6",
    "@vicons/antd": "^0.13.0",
    "axios": "1.6.8",
    "dayjs": "1.11.10",
    "element-plus": "^2.10.2",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "lodash-es": "4.17.21",
    "mitt": "3.0.1",
    "mysql2": "^3.14.1",
    "naive-ui": "^2.42.0",
    "normalize.css": "8.0.1",
    "npm-run-all": "^4.1.5",
    "nprogress": "0.2.0",
    "papaparse": "^5.5.2",
    "path-browserify": "1.0.1",
    "path-to-regexp": "6.2.1",
    "pinia": "2.1.7",
    "rimraf": "^6.0.1",
    "screenfull": "6.0.2",
    "vis-network": "^9.1.13",
    "vue": "3.4.21",
    "vue-router": "4.3.0",
    "wait-on": "^8.0.3",
    "webdriverio": "^9.17.0",
    "xe-utils": "3.5.22"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@commitlint/cz-commitlint": "^18.6.1",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.11.30",
    "@types/nprogress": "0.2.3",
    "@types/path-browserify": "1.0.2",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/test-utils": "2.4.5",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.6",
    "electron": "^37.1.0",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.23.0",
    "icon-gen": "^5.0.0",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "prettier": "^3.2.5",
    "sass": "1.72.0",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "5.4.3",
    "unocss": "0.58.6",
    "vite": "^6.3.5",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-svg-loader": "5.1.0",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "2.0.7"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,scss,html,md}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "keywords": [
    "vue",
    "vue3",
    "admin",
    "vue-admin",
    "vue3-admin",
    "vite",
    "vite-admin",
    "element-plus",
    "element-plus-admin",
    "ts",
    "typescript"
  ],
  "license": "MIT",
  "build": {
    "appId": "com.swhy-fe.app",
    "productName": "Swhy-FE",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "dist",
        "to": "dist"
      }
    ],
    "icon": "build/icon.png",
    "mac": {
      "target": [
        "dmg"
      ],
      "category": "public.app-category.developer-tools",
      "icon": "build/icons/icon.icns",
      "darkModeSupport": true
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "build/icons/icon.ico",
      "requestedExecutionLevel": "asInvoker"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "build/icon.png",
      "category": "Development"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Swhy-FE"
    },
    "dmg": {
      "title": "Swhy-FE Installer",
      "icon": "build/icons/icon.icns",
      "background": "build/background.png",
      "window": {
        "width": 540,
        "height": 380
      }
    }
  }
}
